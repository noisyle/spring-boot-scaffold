<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.chinanx.springboot.repository.HelloRepository" >
  <select id="getDailyReport" parameterType="String" resultType="String">
      SELECT
        content
      FROM
        daily_report
      WHERE
        report_date = #{date}
  </select>
</mapper>
